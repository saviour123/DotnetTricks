using System;

namespace ArrayApplication {
    class MyArray{

        static void Main(string[] args){

            int [] n = new int[10];
            for (int i = 0; i < 10; i++){
                n[i] = i + 100;
            }

            foreach (int item in n)
            {
                int i = item - 100;
                Console.WriteLine("Element[{0}] = {1}", i, item);
            }

            Console.ReadKey();
        }

    }
}